    "accounts",
    "ethers",
    "smart-contracts",
    "ethereum",
    "blockchain",
    "wow"
  ],
  "repository": "github:solidstate-network/hardhat-accounts",
  "license": "MIT",
  "author": "Nick Barry",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
