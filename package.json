  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^24.0.1",
    "hardhat": "^3.0.0-next.15",
    "husky": "^9.1.7",
    "knip": "^5.61.0",
    "lint-staged": "github:ItsNickBarry/lint-staged#knip",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.15",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
